{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "button-type": "off",
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off",
        "frame-title": "off"
      }
    ],
    "disown-opener": "off",
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "axe/structure": "off",
    "highest-available-document-mode": "off",
    "compat-api/html": "off",
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "-webkit-box-reflect"
        ]
      }
    ]
  }
}